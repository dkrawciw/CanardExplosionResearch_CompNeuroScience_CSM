   "source": [
    "def FitzHugh_Nagumo(t, u, a=5, tau_n=60, I=0):\n",
    "  v = u[0]\n",
    "  n = u[1]\n",
    "\n",
    "  dvdt = v - v**3 / 3 - n + I\n",
    "  dndt = (a*v - n) / tau_n\n",
    "\n",
    "  return np.array([dvdt, dndt])"
   ]
